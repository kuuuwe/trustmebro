test_that("purge_string handles empty data frame correctly", {
df <- data.frame(
name = character(0),
description = character(0),
stringsAsFactors = FALSE
)
expected_df <- df
df_modified <- purge_string(df, replacement = "#")
expect_equal(df_modified, expected_df)
})
